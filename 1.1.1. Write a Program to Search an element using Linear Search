int read1(int a[], int n) {
	printf("Enter %d elements : ",n);
	for (int i = 0; i<n; i++) {
		scanf("%d",&a[i]);
	}
	return 0;
}

int linearSearch(int a[], int low, int high, int key){
	while (low <= high){
		if (key == a[low]){
			return low;
		}else{
			return linearSearch(a, low+1,high,key);
		}
	}
	return -1;
}
